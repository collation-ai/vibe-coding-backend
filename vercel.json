{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "api/index.py"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/api/tables/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/api/tables",
      "dest": "api/index.py"
    },
    {
      "src": "/api/data/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/api/query",
      "dest": "api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "api/index.py"
    }
  ]
}