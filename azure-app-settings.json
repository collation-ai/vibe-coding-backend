{
  "name": "vibe-coding-backend",
  "settings": [
    {
      "name": "MASTER_DB_URL",
      "value": "@Microsoft.KeyVault(SecretUri=https://your-keyvault.vault.azure.net/secrets/master-db-url/)",
      "slotSetting": false
    },
    {
      "name": "AZURE_DB_HOST",
      "value": "your-server.postgres.database.azure.com",
      "slotSetting": false
    },
    {
      "name": "ENCRYPTION_KEY",
      "value": "@Microsoft.KeyVault(SecretUri=https://your-keyvault.vault.azure.net/secrets/encryption-key/)",
